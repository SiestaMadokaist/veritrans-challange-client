{
  "name": "react-starter-es6-babel",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > public/bundle.js",
    "start": "ecstatic -p 8000 public",
    "watch": "watchify main.js -o public/bundle.js -dv"
  },
  "dependencies": {
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "ecstatic": "^0.8.0",
    "react": "^0.14.3",
    "react-addons-linked-state-mixin": "^0.14.3",
    "react-inline-css": "^2.0.1",
    "superagent": "^1.4.0",
    "superagent-progress": "0.0.3",
    "uglify-js": "^2.4.24",
    "underscore": "^1.8.3",
    "watchify": "^3.2.3"
  },
  "devDependencies": {
    "babelify": "^6.4.0",
    "css-loader": "^0.23.0",
    "ecstatic": "^0.8.0",
    "style-loader": "^0.13.0",
    "watchify": "^3.6.1"
  }
}
